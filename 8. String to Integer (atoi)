public class Solution {
    public int myAtoi(String str) {
        int l=str.length();
        int sign = 0;
        int result = 0;
        int index = 0;
        
        
        if(l==0)  return 0;
        if(str.charAt(0)=='+'){
            sign = 1;
            index++;
        }else if(str.charAt(0)=='-'){
            sign = -1;
            index++;
        }else{
            sign = 1;
        }
    }
}
